\name{pcext}
\alias{pcext}
\title{Principal Components Extraction}
\description{Extracts first k principal components from the matrix XX (dim 't x n'), loadings are normalized so that lam'lam/n=I, fac is t*k, lam is n*k.}
\usage{pcext(XX,k)}
\arguments{
		\item{XX}{ }
		\item{k}{ }
        }        
\references{
		- "A Note on the Diffusion of Business Cycles," Guerrero-Escobar, S. and Martinez-Ovando, J. C. (2015).

		- "Structured Dynamic Factor Modeling to Measure Nested Business Cycles," Guerrero-Escobar, S. and Martinez-Ovando, J. C. (2015).
		}
\author{
		Juan Carlos Martinez-Ovando <juan.martinez@banxico.org.mx>, <jc.martinez.ovando@gmail.com>
       }